    <esri:MarkerSymbol x:Key="MSShelter" OffsetX="100" OffsetY="110">
        <esri:MarkerSymbol.ControlTemplate>
            <ControlTemplate>
                <Grid x:Name="grid" Width="200">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="65"></RowDefinition>
                        <RowDefinition Height="60"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Border Grid.Row="0" Width="{Binding}" Background="Green" CornerRadius="10" BorderBrush="Black" BorderThickness="1">
                        <TextBlock Grid.Row="0" TextAlignment="Center" Text="{Binding Attributes[MC]}" Foreground="White" FontSize="48" Margin="0"></TextBlock>
                    </Border>
                    <Ellipse Grid.Row="1" Fill="Red" Height="60" Width="60" VerticalAlignment="Center" HorizontalAlignment="Center"></Ellipse>
                </Grid>
            </ControlTemplate>
        </esri:MarkerSymbol.ControlTemplate>
    </esri:MarkerSymbol>

