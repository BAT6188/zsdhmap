                                <Storyboard RepeatBehavior="Forever">
                                    <DoubleAnimation BeginTime="0:0:0" Storyboard.TargetName="Element" Storyboard.TargetProperty="StrokeDashOffset" To="1000" Duration="0:3:0" />
                                </Storyboard>
                            </VisualState>
                        </VisualStateGroup>
                    </VisualStateManager.VisualStateGroups>
                    <!--For polyline and polygon template, a Path lement with the name "Element" is required-->
                    <Path x:Name="Element" StrokeDashArray="2,2" StrokeDashOffset="0" Stroke="#FFF59623" StrokeThickness="24" />
                </Grid>
            </ControlTemplate>
        </esri:LineSymbol.ControlTemplate>
