<UserControl x:Class="zsdpmap.infowindows"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" FontStretch="Expanded" FontSize="16" OpacityMask="#FF305050">
	<UserControl.Background>
		<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="#FF0E5C50" Offset="0.351"/>
			<GradientStop Color="White" Offset="1"/>
		</LinearGradientBrush>
	</UserControl.Background>
    <Grid x:Name="maingrid">
        <WebBrowser Name="wbs"  HorizontalAlignment="Stretch" Margin="0,50,0,0" VerticalAlignment="Stretch" OpacityMask="#FF3884A0" Opacity="0.9">
        	<WebBrowser.Effect>
        		<DropShadowEffect/>
        	</WebBrowser.Effect>
        </WebBrowser>
        <Label Content="案件" Height="28" Name="lbsTitle" VerticalAlignment="Top" HorizontalAlignment="Stretch" Margin="0,22,0,0" Background="#FFF2E6E6" FontSize="14" FontStretch="Normal" FontStyle="Normal" FontWeight="Normal" >
        	<Label.Effect>
        		<DropShadowEffect BlurRadius="21" ShadowDepth="26"/>
        	</Label.Effect>
        </Label>
    </Grid>
</UserControl>
